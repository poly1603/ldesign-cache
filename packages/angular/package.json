{
  "name": "@ldesign/cache-angular",
  "version": "0.2.0",
  "description": "Angular adapter for @ldesign/cache-core with services",
  "keywords": [
    "cache",
    "angular",
    "services",
    "ldesign"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:clean": "ldesign-builder clean && ldesign-builder build -f esm,cjs,dts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0 || ^18.0.0",
    "@ldesign/cache-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@angular/core": "^19.1.4",
    "@types/node": "^20.0.0",
    "typescript": "^5.7.2",
    "rxjs": "^7.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}

