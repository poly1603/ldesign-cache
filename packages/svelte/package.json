{
  "name": "@ldesign/cache-svelte",
  "version": "0.2.0",
  "description": "Svelte adapter for @ldesign/cache-core with stores",
  "keywords": [
    "cache",
    "svelte",
    "stores",
    "ldesign"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs",
      "svelte": "./es/index.js"
    }
  },
  "svelte": "./es/index.js",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:clean": "ldesign-builder clean && ldesign-builder build -f esm,cjs,dts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0",
    "@ldesign/cache-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.7.2",
    "svelte": "^5.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

