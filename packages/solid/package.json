{
  "name": "@ldesign/cache-solid",
  "version": "0.2.0",
  "description": "Solid adapter for @ldesign/cache-core with reactive stores",
  "keywords": [
    "cache",
    "solid",
    "solidjs",
    "reactive",
    "ldesign"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:clean": "ldesign-builder clean && ldesign-builder build -f esm,cjs,dts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0",
    "@ldesign/cache-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.0.0",
    "typescript": "^5.7.2",
    "solid-js": "^1.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}

