{
  "name": "@ldesign/cache-vue",
  "version": "1.0.0",
  "description": "企业级缓存管理库 - Vue 3 适配器",
  "keywords": [
    "cache",
    "vue",
    "vue3",
    "composable",
    "ldesign",
    "typescript"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist es lib"
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.3",
    "rimraf": "^5.0.0",
    "vue": "^3.3.0"
  }
}

