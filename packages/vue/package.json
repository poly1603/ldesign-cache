{
  "name": "@ldesign/cache-vue",
  "version": "0.2.0",
  "description": "Vue3 adapter for @ldesign/cache-core with reactive composables",
  "keywords": [
    "cache",
    "vue",
    "vue3",
    "composition-api",
    "hooks",
    "ldesign"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/cache-vue.min.js",
  "jsdelivr": "./dist/cache-vue.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:clean": "ldesign-builder clean && ldesign-builder build -f esm,cjs,dts",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --fix",
    "lint:check": "eslint ."
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "@ldesign/cache-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.13",
    "vue-tsc": "^3.0.5",
    "eslint": "^9.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
