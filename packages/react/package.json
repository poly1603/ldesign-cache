{
  "name": "@ldesign/cache-react",
  "version": "0.2.0",
  "description": "React adapter for @ldesign/cache-core with hooks and context",
  "keywords": [
    "cache",
    "react",
    "hooks",
    "context",
    "ldesign"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./hooks": {
      "types": "./es/hooks/index.d.ts",
      "import": "./es/hooks/index.js",
      "require": "./lib/hooks/index.cjs"
    },
    "./hooks/*": {
      "types": "./es/hooks/*.d.ts",
      "import": "./es/hooks/*.js",
      "require": "./lib/hooks/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/cache-react.min.js",
  "jsdelivr": "./dist/cache-react.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:clean": "ldesign-builder clean && ldesign-builder build -f esm,cjs,dts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint . --fix",
    "lint:check": "eslint ."
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@ldesign/cache-core": "workspace:*"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "eslint": "^9.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
