{
  "name": "@ldesign/cache",
  "version": "0.2.0",
  "description": "高性能缓存管理库 - 支持多存储引擎、智能策略、安全特性和 Vue 3 深度集成",
  "keywords": [
    "cache",
    "storage",
    "indexeddb",
    "localstorage",
    "memory-cache",
    "ldesign",
    "vue3",
    "typescript",
    "framework-agnostic"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/cache#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/cache"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:core": "pnpm --filter @ldesign/cache-core run build",
    "build:vue": "pnpm --filter @ldesign/cache-vue run build",
    "build:watch": "pnpm --filter \"./packages/*\" run build:watch",
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "type-check": "pnpm --filter \"./packages/*\" run type-check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "pnpm --filter \"./packages/*\" run clean && rimraf es lib dist"
  },
  "dependencies": {
    "@ldesign/cache-core": "workspace:*",
    "@ldesign/cache-vue": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "private": true
}